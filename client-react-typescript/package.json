{
  "name": "apollo-offline-examples",
  "version": "0.0.0",
  "description": "Examples for apollo-offline",
  "homepage": "https://github.com/Malpaux/apollo-offline-examples#readme",
  "bugs": {
    "url": "https://github.com/Malpaux/apollo-offline-examples"
  },
  "license": "BSD-3-Clause",
  "private": true,
  "author": "Malpaux (malpaux.com)",
  "contributors": [
    {
      "name": "Paul Brachmann",
      "email": "paul.brachmann@malpaux.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Malpaux/apollo-offline-examples.git"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json --fix",
    "start": "node scripts/start.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:coverage": "node scripts/test.js --env=jsdom --coverage"
  },
  "dependencies": {
    "@redux-offline/redux-offline": "^2.3.1",
    "apollo-offline": "^0.1.0",
    "csx": "^8.5.0",
    "react": "^16.2.0",
    "react-apollo": "^1.4.16",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.0",
    "react-typestyle": "^0.3.3",
    "react-typestyle-inline": "^0.3.3",
    "react-typestyle-preset": "^0.3.2",
    "redux": "^3.7.2",
    "theming": "^1.3.0",
    "typestyle": "^1.7.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.8",
    "@types/enzyme-adapter-react-16": "^1.0.1",
    "@types/jest": "^22.1.2",
    "@types/node": "^9.4.5",
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.14",
    "@types/react-router-dom": "^4.2.3",
    "@types/redux": "^3.6.0",
    "@types/webpack-env": "^1.13.5",
    "awesome-typescript-loader": "^3.4.1",
    "babel-polyfill": "^6.26.0",
    "browserconfig-loader": "^0.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.1.1",
    "chalk": "^2.3.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-loader": "^1.0.2",
    "file-loader": "^1.1.6",
    "fs-extra": "^5.0.0",
    "graphql-tag": "^2.7.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "img-loader": "^2.0.1",
    "jest": "^22.2.2",
    "jest-localstorage-mock": "^1.2.0",
    "jest-transform-graphql": "^2.1.0",
    "react-dev-utils": "^5.0.0",
    "react-error-overlay": "^4.0.0",
    "react-test-renderer": "^16.2.0",
    "sw-precache-webpack-plugin": "^0.11.4",
    "ts-jest": "^22.0.3",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.6.0",
    "tslint-loader": "^3.5.3",
    "tslint-react": "^3.4.0",
    "typescript": "^2.7.1",
    "url-loader": "^0.6.2",
    "web-manifest-loader": "^0.0.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "mapCoverage": true,
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "\\.gql$"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "<rootDir>/(build|dist|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost"
  }
}
